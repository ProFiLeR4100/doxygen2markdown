{
	"name": "doxygen2markdown",
	"version": "0.0.1",
	"description": "Converts Doxygen XML to MD files.",
	"main": "./lib/index.js",
	"bin": {
		"doxygen2markdown": "./lib/index.js"
	},
	"scripts": {
		"start": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts",
		"create": "npm run build && npm run test",
		"build": "tsc -p .",
		"refresh": "npm i -g"
	},
