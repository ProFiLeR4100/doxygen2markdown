	"bugs": {
		"url": "https://github.com/ProFiLeR4100/doxygen2markdown/issues"
	},
	"homepage": "https://github.com/ProFiLeR4100/doxygen2markdown#readme",
	"dependencies": {
		"chalk": "^3.0.0",
		"clear": "^0.1.0",
		"commander": "^4.1.0",
		"ejs": "^3.0.1",
		"figlet": "^1.2.4",
		"path": "^0.12.7",
		"xml2json": "^0.12.0"
	},
	"devDependencies": {
		"@types/node": "^13.5.0",
